FUNC VOID Use_Bookstand_Background_01_S1()		
{
	var C_NPC her; 	her = Hlp_GetNpc(PC_Hero); 
	
	if  (Hlp_GetInstanceID(self) == Hlp_GetInstanceID(her))
	{	

		if (BackGround_01 == 0)
		{
			BackGroundCounter += 1;

			BackGround_01 = BackGroundCounter;

			B_GivePlayerXP	(XP_Bookstand);
		};

		B_ShowBackground_Story (BackGround_01);
	};
};

